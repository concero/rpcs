{
  "name": "concero-rpcs",
  "version": "1.0.0",
  "description": "A service for upkeeping a list of healthy RPC endpoints for multiple chains",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js"
  ],
  "scripts": {
    "start": "bun ./src/index.ts --run-once",
    "build": "tsc ./index.ts",
    "clean:output": "rm -rf output",
    "clean:logs": "rm -rf logs",
    "clean": "npm run clean:output && npm run clean:logs",
    "build:types": "tsc ./index.ts --declaration --emitDeclarationOnly",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "format": "prettier --config .prettierrc.json --write \"src/**/*.ts\" \"**/*.json\"",
    "prepare": "husky",
    "probe:finality": "bun ./src/scripts/finalityProbe.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concero/rpcs.git"
  },
  "keywords": [
    "rpcs"
  ],
  "author": "Concero GMBH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concero/rpcs/issues"
  },
  "homepage": "https://github.com/concero/rpcs#readme",
  "dependencies": {
    "acorn": "^8.14.0",
    "acorn-walk": "^8.3.4",
    "node-cron": "^3.0.3",
    "simple-git": "^3.27.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "22.14.1",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "typescript": "^5.3.3"
  }
}
